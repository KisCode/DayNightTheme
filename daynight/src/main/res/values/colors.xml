<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--  主题颜色，button/toolbar 默认背景颜色  -->
    <color name="colorPrimary">#FFAA86BC</color>
    <color name="colorPrimaryDark">#FFA166EE</color>
    <!--  主题控件上显示颜色，button/toolbar上文字颜色  -->
    <color name="colorOnPrimary">#FFFFFF</color>

    <!--  交互性颜色，如RadioButton/CheckBox 勾选颜色等  -->
    <color name="colorSecondary">#FF03DAC6</color>
    <color name="colorSecondaryVariant">#FF018786</color>
    <color name="colorOnSecondary">#FF000000</color>

    <color name="colorBackground">#FFFFFFFF</color>
    <color name="colorOnBackground">#FF000000</color>

    <!--  用于表单或表面 (如卡片颜色和应用的底部弹出菜单颜色) ,这两个值合成radioButton选中框的颜色？ -->
    <color name="colorSurface">#FFFFFFFF</color>
    <color name="colorOnSurface">#FF000000</color>

    <!--  错误颜色  -->
    <color name="colorError">#FFFF0000</color>
    <color name="colorOnError">#FFFFFF</color>

    <color name="textColor">#FF3C3C3C</color>

</resources>